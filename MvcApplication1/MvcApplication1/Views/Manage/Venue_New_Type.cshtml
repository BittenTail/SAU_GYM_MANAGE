@using MvcApplication1.Models
@model Venue
@{
    ViewBag.Title = "Venue_New_Type";
    Layout = "~/Views/Shared/_Layout_Venue_Manage.cshtml";
}

<h2>添加新场地类型</h2>
<div style="padding-left:15px">
    @using (Html.BeginForm("Venue_New_Type", "Manage", FormMethod.Post))
    {
        <b>新类型名称：@Html.TextBoxFor(m => m.typename)</b>
        <br />
        <b>@Html.ValidationMessageFor(m => m.typename)</b>
        <input type="submit" value="添加类型" />
    }
</div>
